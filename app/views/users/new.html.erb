<%= form_for @user do |f| %>

	<h4><%= f.label :name %></h4>
	<h4><%= f.text_field :name %><h4>

	<h4><%= f.label :email %><h4>
	<h4><%= f.email_field :email %><h4>

	<h4><%= f.label :password %><h4>
	<h4><%= f.password_field :password%><h4>

	<h4><%= f.label "Password Confirmation" %><h4>
	<h4><%= f.password_field :password_confirmation %><h4>

	<%= f.submit "Create Account" %>

<% end %>
