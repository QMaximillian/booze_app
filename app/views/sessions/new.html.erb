<h3>Login:</h3>
<%= form_tag sessions_path do %>
  <%= label_tag "Email" %>
  <%= text_field_tag :email %><br>
  <%= label_tag "Password" %>
  <%= password_field_tag :password %><br>
  <%= submit_tag "Login" %><br>

  <p>Create an account here</p>
  <h5><%= link_to "Sign Up", new_user_path %>
<% end %>
